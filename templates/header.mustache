{{!
    Page navbar.
}}

  <header id="page-wrapper-header" class="fixed-top ">
    <div id="page-header">
        <div class="clearfix conteiner-fluid  ">

            <!--above navbar-->
           <nav class="navbar navbar-dark bg-ufma text-white navbar-expand moodle-has-zindex" aria-label="{{#str}}sitemenubar, admin{{/str}}">



                 <a href="{{{ config.wwwroot }}}" class="navbar-brand aabtn {{# output.has_compact_logo }}has-logo{{/ output.has_compact_logo }}
                    {{^ output.has_compact_logo }}
                        d-none d-sm-inline
                    {{/ output.has_compact_logo }}
                        ">
                    {{# output.has_compact_logo }}
                        <span id="ufma-logo" class=" d-none d-sm-inline">
                            <img src="{{output.get_compact_logo_url}}" alt="{{sitename}}">
                        </span>
                    {{/ output.has_compact_logo }}
                </a>

               

                 <div id="sitetitle" class="text-white mx-auto">
            <h1>{{{ sitename }}}</h1>

        </div>

                <ul class="navbar-nav d-none d-md-flex">
                    <!-- custom_menu -->
                    <!-- page_heading_menu -->
                    {{{ output.page_heading_menu }}}
                </ul>
                <ul class=" nav navbar-nav ml-auto">
                    <li class="d-none d-lg-block">
                        {{{ output.search_box }}}
                    </li>
                    <!-- navbar_plugin_output -->
                    <li class="nav-item">
                        {{{ output.navbar_plugin_output }}}
                    </li>
                    <!-- user_menu -->
                    <li class="nav-item d-flex align-items-center">
                        {{{ output.user_menu }}}
                    </li>
                </ul>
                    <!-- search_box -->
            </nav>

            


        </div>

    </div>
    
    <div class="navwrap container-fluid">
        <div class="row ">
            <nav class=" col navbar navbar-expand-lg navbar-light bg-light text-dark"   style="position: relative;">
                        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse " id="navbarNav" style="background:white">
                        <div class="navbar-nav" id="mainNav">
                            <a> <div data-region="drawer-toggle" class="d-inline-block mr-3 btn-sidebar {{#drawer-open-left }}open{{/drawer-open-left }}" id="btn-sidebar">

                                <button aria-expanded="{{#navdraweropen}}true{{/navdraweropen}}{{^navdraweropen}}false{{/navdraweropen}}" aria-controls="nav-drawer" type="button" class="btn nav-link float-sm-left mr-1 btn-light bg-gray" data-action="toggle-drawer" data-side="left" data-preference="drawer-open-nav">{{#pix}}i/menubars{{/pix}}<span class="sr-only">{{#str}}sidepanel, core{{/str}}</span></button>

                            </div></a>
                           
                            {{{ output.topmenu }}}
                        
                        </div>
                        </div>
                    </nav>
        </div>

    </div>
   
</header>





